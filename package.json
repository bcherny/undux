{
  "name": "undux",
  "version": "3.1.0",
  "description": "Dead simple state management for React",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "author": "Boris Cherny <boris@performancejs.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/bcherny/undux.git"
  },
  "bugs": {
    "url": "https://github.com/bcherny/undux/issues"
  },
  "homepage": "https://github.com/bcherny/undux#readme",
  "scripts": {
    "build": "npm run clean; npm run lint && tsc -d && cp src/index.js.flow dist/src/index.js.flow && flow check test/test.flow.js",
    "clean": "shx rm -rf dist",
    "lint": "tslint src/**/*",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "tdd": "concurrently -k 'tsc -w' 'ava -w'",
    "test": "ava && flow check src/index.js.flow"
  },
  "dependencies": {
    "typed-rx-emitter": "^0.3.1"
  },
  "devDependencies": {
    "@types/jsdom": "^11.0.4",
    "@types/react": "^16.0.34",
    "@types/react-dom": "^16.0.3",
    "ava": "^0.24.0",
    "concurrently": "^3.5.1",
    "flow-bin": "^0.64.0",
    "jsdom": "^11.5.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "shx": "^0.2.2",
    "tslint": "^5.9.1",
    "tslint-react": "^3.4.0",
    "typescript": "^2.6.2"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "keywords": [
    "redux",
    "flux",
    "rx",
    "reactive",
    "simple",
    "model",
    "state",
    "store"
  ],
  "ava": {
    "files": [
      "./dist/test/test.js"
    ],
    "source": [
      "./dist/src/*.js"
    ],
    "vebose": true
  }
}
